<script type="text/javascript" id="mbscript">
$script.ready('bundle', function() {
	$script.ready('voting', function() {
		$(document).ready(function() {
			// --- Vote Rating
			var $vote_input = $('.download_vote input.rating');

			if ($vote_input.length) {
				$vote_input.removeClass('rating-loading').addClass('rating-loading').rating();
			}
			
			
			
				$vote_input.rating('refresh', {showClear:false, disabled:true, showCaption:false});
				$vote_input.each(function() {
					var vote = $(this).data('vote');
					$(this).prev().css('width', vote + '%');
				});
			
		
			$('.rating-stars').each(function() {
				$(this).click(function() {
					var rate = ($(this).css('width').match(/\d+/) / $(this).parent().css('width').match(/\d+/)) * 100 / 20;
					var vboardId = $(this).next().data('boardid');
					var userId = 206273;
					var dataObj = { 'vote': Math.round(rate), 'boardId': vboardId, 'userId': userId };
					var self = $(this);
					$.post('https://www.soundboard.com/VotingService.asmx/Voting', dataObj).done(function(result) {
						var data = $.parseJSON(result.documentElement.innerHTML);
							self.next().rating('refresh', { showClear: false, disabled: true, showCaption: false });
							self.css("width", data._Vote + "%");
					});
					return false;
				});
			}); 

			$(".star-rating.rating-xs.rating-active .caption span").each(function() {
				var vote = $(this).parent().prev().find('input').data('vote');

				if (vote > 0 && vote <= 20) {
					$(this).removeClass('label-default');
					$(this).addClass('label-danger');
				}
				if (vote > 20 && vote <= 40){
					$(this).removeClass('label-default');
					$(this).addClass('label-warning');
				}
				if (vote > 40 && vote <= 60){
					$(this).removeClass('label-default');
					$(this).addClass('label-info');
				}
				if (vote > 60 && vote <= 80){
					$(this).removeClass('label-default');
					$(this).addClass('label-primary');
				}
				if (vote > 80 && vote <= 100){
					$(this).removeClass('label-default');
					$(this).addClass('label-success');
				}
			});

			$(".star-rating.rating-xs.rating-active .rating-stars").each(function () {
				var vote = $(this).next().data('vote');
				var userVoted = $(this).next().data('uservoted');
				if (userVoted == "True") {
					$(this).next().rating('refresh', { showClear: false, disabled: true, showCaption: false });
					$(this).css('width', vote + '%');
				} else {
					$(this).css('width', vote + '%');
					$(this).next().rating('update', vote / 20);
				}
			});	
		});
	});
});


</script>
